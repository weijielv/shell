<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 填充的颜色 -->
    <solid android:color="@color/c11_f0eff4" />
    <!-- 设置按钮的四个角为弧形 -->
    <!-- android:radius 弧形的半径 -->
    <corners
        android:bottomLeftRadius="@dimen/dimen_10px"
        android:bottomRightRadius="@dimen/dimen_10px"
        android:topLeftRadius="@dimen/dimen_10px"
        android:topRightRadius="@dimen/dimen_10px" />

    <!-- 边框宽度，颜色值 -->
    <stroke
        android:width="1px"
        android:color="@color/c09_979ca8" />

</shape>